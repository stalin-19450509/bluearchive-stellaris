entity = {
	name = "ba_mega_pyroxenes_base_stage_1_entity"
	pdxmesh = "matter_decompressor_01_stage_1_mesh"
	
		meshsettings = {
			name = "g_body1Shape"
			index = 0
			texture_diffuse = "ba_mega_pyroxenes_base_diffuse.dds"
			texture_normal = "ba_mega_pyroxenes_base_normal.dds"
			texture_specular = "ba_mega_pyroxenes_base_specular.dds"
			shader = "PdxMeshShip"
		}
	
	default_state = "idle"
	state = { name = "idle" animation = "idle"
		start_event = { node = "effect_1" particle = "matde_core_beam_effect" keep_particle = yes trigger_once = yes }
		
